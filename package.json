{
  "name": "depngn",
  "version": "0.3.0",
  "description": "Determine the compatibility of your packages with a given Node version",
  "author": "Lewis D'Avanzo",
  "license": "MIT",
  "bugs": "https://github.com/upgradejs/depngn/issues",
  "repository": "github:upgradejs/depngn",
  "keywords": [
    "engines",
    "dependencies",
    "upgrade",
    "version",
    "semver",
    "cli"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "depngn": "bin/depngn.cjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "predev": "npm run build",
    "dev": "./bin/depngn.cjs",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "compare-versions": "^5.0.1",
    "kleur": "^4.1.5",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.7.19",
    "@types/semver": "^7.3.12",
    "@types/signal-exit": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "signal-exit": "^3.0.7",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
